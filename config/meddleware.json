{
    "morgan": {
        "enabled": true,
        "priority": 5,
        "module": {
            "name": "morgan",
            "arguments": [
                "combined"
            ]
        }
    },
    "bodyParserJSON": {
        "enabled": true,
        "priority": 10,
        "module": {
            "name": "body-parser",
            "method": "json",
            "arguments": [
                {
                    "limit": "50mb"
                }
            ]
        }
    },
    "bodyParserUrlEncoding": {
        "enabled": true,
        "priority": 20,
        "module": {
            "name": "body-parser",
            "method": "urlencoded",
            "arguments": [
                {
                    "extended": true,
                    "limit": "50mb"
                }
            ]
        }
    },
    "auth": {
        "enabled": true,
        "priority": 40,
        "module": {
            "name": "app/api/middleware/auth"
        }
    },
    "enrouten": {
        "enabled": true,
        "priority": 50,
        "module": {
            "name": "express-enrouten",
            "arguments": [
                {
                    "index": "path:../lib/app/api/routes/index",
                    "routes": "config:routes"
                }
            ]
        }
    },
    "error": {
        "enabled": true,
        "priority": 60,
        "module": {
            "name": "app/api/middleware/error"
        }
    },
    "compression": {
        "enabled": true,
        "priority": 70,
        "module": {
            "name": "compression"
        }
    }
}
